@page
@model HormonaCrecimiento.App.Pages_BDPacientes.DetailsModel
@{
}


<div class="col-auto p-5">
    <h2 class="text-white">Detalle del Paciente</h2>
    <dl class="row text-white">
      <dt class="col-sm-2">Nombre</dt>
      <dd class="col-sm-10">@Model.Paciente.Nombres</dd>
      <dt class="col-sm-2">Apellido</dt>
      <dd class="col-sm-10">@Model.Paciente.Apellidos</dd>
      <dt class="col-sm-2">Telefono</dt>
      <dd class="col-sm-10">@Model.Paciente.NumeroTelefono</dd>
      <dt class="col-sm-2">Genero</dt>
      <dd class="col-sm-10">@Model.Paciente.Genero</dd>
      <dt class="col-sm-2">Fecha de Nacimiento</dt>
      <dd class="col-sm-10">@Model.Paciente.FechaNacimiento</dd>
      <dt class="col-sm-2">Documento</dt>
      <dd class="col-sm-10">@Model.Paciente.Documento</dd>
      <dt class="col-sm-2">Direccion</dt>
      <dd class="col-sm-10">@Model.Paciente.Direccion</dd>
      <dt class="col-sm-2">Latitud</dt>
      <dd class="col-sm-10">@Model.Paciente.Latitud</dd>
      <dt class="col-sm-2">Longitud</dt>
      <dd class="col-sm-10">@Model.Paciente.Longitud</dd>
      <dt class="col-sm-2">Ciudad</dt>
      <dd class="col-sm-10">@Model.Paciente.Ciudad</dd>
      <dt class="col-sm-2">Medico</dt>
      <dd class="col-sm-10">@Model.Paciente.Medico.Nombres</dd>
    </dl>
  
</div>
